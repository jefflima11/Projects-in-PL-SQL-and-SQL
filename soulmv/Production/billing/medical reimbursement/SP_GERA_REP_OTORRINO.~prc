CREATE OR REPLACE PROCEDURE SP_GERA_REP_OTORRINO(
	PDT_REPASSE IN DATE,
	PDS_REPASSE IN VARCHAR2,
	PCD_REPASSE OUT NUMBER
)
IS
BEGIN
  INSERT INTO DBAMV.REPASSE(
		CD_REPASSE,
		DT_COMPETENCIA,
		DT_REPASSE,
		DS_REPASSE,
		TP_REPASSE,
		CD_MULTI_EMPRESA,
		SN_CONTABILIZAR
	) VALUES (
		SEQ_REPASSE.NEXTVAL,
		PDT_REPASSE,
		PDT_REPASSE,
		PDS_REPASSE,
		'M',
		1,
		'N'
	)
	RETURNING CD_REPASSE INTO PCD_REPASSE;
END SP_GERA_REP_OTORRINO;
/

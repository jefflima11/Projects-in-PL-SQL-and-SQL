SELECT SUM(INTERNACOES),
       SUM(OBITO),
	   TRUNC((SUM(OBITO)/SUM(INTERNACOES))*100) TX_OBITO
	   
FROM   (
SELECT COUNT(*) INTERNACOES,
       0 OBITO

FROM    DBAMV.ATENDIME A

WHERE   A.TP_ATENDIMENTO = 'I'
  AND   A.DT_ALTA IS NOT NULL
  AND   TO_CHAR(A.DT_ALTA,'MMYYYY') = '012024'
  
UNION ALL

SELECT 0 INTERNACOES,
       COUNT(*) OBITO

FROM   DBAMV.PW_REGISTRO_ALTA RA,
       DBAMV.MOT_ALT MA
	   
WHERE  RA.CD_MOT_ALT = MA.CD_MOT_ALT
  AND  TO_CHAR(RA.DT_REGISTRO_ALTA,'MMYYYY') = 012024
  AND  MA.DS_MOT_ALT LIKE ('%OBITO%')
       )
;

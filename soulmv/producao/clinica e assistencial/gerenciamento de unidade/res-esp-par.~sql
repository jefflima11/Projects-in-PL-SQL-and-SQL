/*CREATED BY
NAME: JEFFERSON LIMA GONCALVES
CARGO: ANALISTA E DESENVOLVEDOR DE SISTEMAS JR
DATA: 01/02/2024
TITULO: ALERGIAS APOIO
DESCRICAO: LISTAGEM DE ALERGIAS PARA INTEGRAÇÃO DO SISTEMA "APOIO"
FILTROS:

ÚLTIMA ATUALIZAÇÃO
RESPÓNSAVEL:
DATA:
MOTIVO:
DESCRIÇÃO:
*/

SELECT TO_CHAR(PM.DT_SOLICITACAO,'DD/MM/YYYY') AS DT,
       PM.CD_ATENDIMENTO,
       PA.NM_PACIENTE,
       PR.NM_MNEMONICO,
       E.DS_ESPECIALID,
       DECODE(A.TP_ATENDIMENTO,'U','URGENCIA E EMERGENCIA','I','INTERNACAO') AS ORIGEM
FROM PAR_MED PM
     INNER JOIN
     PRESTADOR PR
     ON PR.CD_PRESTADOR = PM.CD_PRESTADOR_PARECER
     INNER JOIN
     ATENDIME A
     ON A.CD_ATENDIMENTO = PM.CD_ATENDIMENTO
     INNER JOIN
     PACIENTE PA
     ON PA.CD_PACIENTE = A.CD_PACIENTE
     INNER JOIN
     ESPECIALID E
     ON E.CD_ESPECIALID = PM.CD_ESPECIALID
WHERE TRUNC(PM.DT_SOLICITACAO) BETWEEN @p_data_in AND @p_data_fim
AND PR.CD_PRESTADOR IN (1023)
ORDER BY PM.DT_SOLICITACAO ASC

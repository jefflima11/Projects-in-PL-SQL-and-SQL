CREATE SEQUENCE SEQ_LOG_MOV_INT_HUMS START WITH 1 INCREMENT BY 1;

CREATE TABLE LOG_MOV_INT_HUMS (
    ID_LOG_MOV_INT NUMBER PRIMARY KEY,
    CD_ATENDIMENTO NUMBER NULL,
    CD_MOV_INT NUMBER NOT NULL,
		CD_CONVENIO NUMBER,
		CD_PRESTADOR NUMBER,
		CD_LEITO NUMBER,
		DT_MOV_INT DATE NOT NULL,
		HR_MOV_INT DATE NOT NULL,
		DS_MOTIVO VARCHAR2(2000),
		SN_RESERVA VARCHAR2(1),
		CD_LEITO_ANTERIOR NUMBER(4),
		CD_TIP_ACOM NUMBER(2),
		TP_MOV VARCHAR2(1),
		NM_USUARIO VARCHAR2(30),
		DT_LIV_MOV DATE,
    HR_LIB_MOV DATE,
		CD_MOTIVO_TRANSF_LEITO NUMBER(3),
		CD_MOV_INT_INTEGRA VARCHAR2(50),
		CD_SEQ_INTEGRA NUMBER(20),
		DT_INTEGRA DATE,
		DS_LOCAL VARCHAR2(60),
		NR_NUMERACAO_ACESSO VARCHAR2(30),
    USUARIO_LOGADO VARCHAR2(30)

);

COMMENT ON TABLE LOG_MOV_INT_HUMS IS 'TABELA DE LOG DOS REGISTROS DA TABELA MOV_INT';
